<!-- users.new.html.erb -->

<h1>Compartment App</h1>
<h4>User Registration</h4>

<%= form_for(@user) do |f| %>
  <%= f.label(:first_name) %>
  <%= f.text_field(:first_name) %>
  <%= f.label(:last_name) %>
  <%= f.text_field(:last_name) %>
  <%= f.label(:email) %>
  <%= f.text_field(:email) %>
  <%= f.label(:password) %>
  <%= f.password_field(:password) %>
  <%= f.label(:password_confirmation) %>
  <%= f.password_field(:password_confirmation) %>
  <%= f.submit %>
<% end %>
