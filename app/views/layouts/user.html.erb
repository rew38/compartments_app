<!-- layouts.user.html.erb -->

<!DOCTYPE html>
<html>
<head>
  <title>TutoringApp</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if session[:user_id] %>
  <a href="/logout"> Logout</a>
<% else %>
  Sign Up Below
<%end%>
<%= yield %>

</body>
</html>
